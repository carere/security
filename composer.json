{
  "name": "ashiso/security",
  "description": "Identity & Access Context",
  "authors": [
    {
      "name": "Kevin Abatan",
      "email": "abatan.k@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ashiso\\Security\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "ext-dom": "*",
    "doctrine/orm": "2.*",
    "illuminate/support": "8.*",
    "illuminate/database": "8.*",
    "ramsey/uuid": "4.*"
  },
  "require-dev": {
    "behat/behat": "3.*",
    "phpunit/phpunit": "9.*",
    "friendsofphp/php-cs-fixer": "2.*",
    "symfony/dependency-injection": "5.*",
    "symfony/config": "5.*",
    "symfony/yaml": "5.*",
    "symfony/dotenv": "5.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ashiso\\Security\\Infrastructure\\Laravel\\SecurityServiceProvider"
      ]
    }
  }
}
