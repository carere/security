{
  "name": "addworking/security",
  "description": "Identity & Access Context",
  "authors": [
    {
      "name": "Kevin Abatan",
      "email": "abatan.k@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Addworking\\Security\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "require": {
    "doctrine/orm": "2.*",
    "illuminate/support": "7.*",
    "illuminate/database": "7.*",
    "ramsey/uuid": "^4.0"
  },
  "require-dev": {
    "behat/behat": "3.*",
    "phpunit/phpunit": "9.*",
    "friendsofphp/php-cs-fixer": "2.*",
    "symfony/dependency-injection": "5.*",
    "symfony/config": "5.*",
    "symfony/yaml": "5.*",
    "symfony/dotenv": "5.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Addworking\\Security\\Infrastructure\\Laravel\\SecurityServiceProvider"
      ]
    }
  }
}
