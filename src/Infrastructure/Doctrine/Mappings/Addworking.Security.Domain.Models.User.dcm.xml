<doctrine-mapping>
    <entity name="Addworking\Security\Domain\Models\User" schema="public" table="addworking_user_users" repository-class="Addworking\Security\Infrastructure\Doctrine\Repositories\UserRepository">

        <id name="id" column="id" type="guid">
            <generator strategy="NONE" />
        </id>
        <field name="firstname" column="firstname" type="string" length="255" />
        <field name="lastname" column="lastname" type="string" length="255" />

    </entity>
</doctrine-mapping>
