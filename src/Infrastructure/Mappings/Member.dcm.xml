<doctrine-mapping>
    <entity name="Addworking\Domain\Models\Member" table="addworking_user_users" schema="public" repository-class="Addworking\Infrastructure\Doctrine\MemberRepository">

        <id name="id" column="id" type="guid">
            <generator strategy="NONE" />
        </id>

        <many-to-one field="user" target-entity="User" />
        <many-to-one field="enterprise" target-entity="Enterprise" />

    </entity>
</doctrine-mapping>
